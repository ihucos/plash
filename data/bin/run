#!/usr/bin/python3
import argparse
import atexit
import os
import subprocess
import sys
import tempfile
import uuid

from plash import utils
from plash.core import Container
from plash.utils import get_subcommand_path

container_id = sys.argv[1]
cmd = sys.argv[2:]

c = Container(container_id)
if not c.is_builded():
    print("Container not builded")
    sys.exit(1)
c.run(cmd)
